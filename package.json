{
  "name": "b2b-lead-generation-platform",
  "version": "1.0.0",
  "description": "AI-powered B2B lead generation platform with Meta integration",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "db:generate": "wrangler d1 execute lead-generation-db --file=./schemas/database.sql",
    "db:seed": "wrangler d1 execute lead-generation-db --file=./schemas/seed.sql"
  },
  "dependencies": {
    "@react-router/cloudflare": "^7.0.0",
    "@react-router/dev": "^7.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router": "^7.6.2",
    "hono": "^4.0.0",
    "lucide-react": "^0.263.1",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "wrangler": "^3.78.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
